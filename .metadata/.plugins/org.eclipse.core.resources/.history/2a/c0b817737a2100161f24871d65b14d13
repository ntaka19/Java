//a container of candies
class CandyContainer extends Acontainer {
  Candy content;

  CandyContainer(Candy content) {
    this.content = content;
  }
  //to take content out
  Candy get() {
    return content;
  }
  
  //to swap the content in this container with the one in that container
  void swap(CandyContainer that) {
    Candy c = this.content;
    this.content = that.content;
    that.content = c;
  }
}
