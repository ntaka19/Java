
public class AContainer {
	Object content;
	
	AContainer(Object content){
		this.content = content;
	}
	
	Object get(){
		return content; //このままだと、objectを返すことになる。objectにはcolorが定義されていない。
	}
	
	void swap(AContainer that){
		Object c = this.content;
		this.content = that.content;
		that.content = c;
	}
}
